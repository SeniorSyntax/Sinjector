<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <RootNamespace>Sinjector</RootNamespace>

        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.0.0.0</PackageVersion>
        <Description>Injector for NUnit tests.</Description>
        <PackageProjectUrl>https://github.com/SeniorSyntax/Sinjector</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageTags>NUnit;DI;Injection;IoC;Autofac</PackageTags>
    </PropertyGroup>
    <ItemGroup>
      <ProjectReference Include="..\Sinjector\Sinjector.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Autofac" Version="7.1.0" />
    </ItemGroup>
</Project>
