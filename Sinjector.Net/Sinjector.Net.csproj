<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <RootNamespace>Sinjector</RootNamespace>

        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.0.0.0</PackageVersion>
        <Description>Injector for .net tests.</Description>
        <PackageProjectUrl>https://github.com/SeniorSyntax/Sinjector</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageTags>NUnit;DI;Injection;IoC</PackageTags>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\Sinjector\Sinjector.csproj" />
    </ItemGroup>
    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.8" />
    </ItemGroup>
</Project>